{
  "name": "share_it",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "ios": "react-native run-ios",
    "android": "react-native run-android",
    "android-i": "react-native run-android --interactive",
    "android-l": "react-native run-android --list-devices",
    "build-android": "react-native build-android",
    "lint": "eslint .",
    "clean-ios": "cd ios && xcodebuild clean",
    "clean-android": "cd android && gradlew clean",
    "clean-npm": "rd /s /q node_modules && npm cache clean --force",
    "clean-metro": "rmdir /s /q %TEMP%\\metro-cache",
    "clean-jest": "rmdir /s /q %TEMP%\\haste-map-*",
    "clean-watchman": "watchman watch-del-all",
    "reset-cache": "react-native start -- --reset-cache",
    "ultimate-reset": "npm run clean-npm && npm run clean-metro",
    "ultimate-reset2": "npm run clean-npm && npm run clean-watchman && npm run clean-jest && npm run clean-metro",
    "pod-install": "cd ios && RCT_NEW_ARCH_ENABLED=1 bundle exec pod install",
    "install": "npm install js-yaml --no-save && node install.js",
    "test": "jest"
  },
  "dependencies": {
    "@react-navigation/native-stack": "^7.2.0",
    "buffer": "^6.0.3",
    "immer": "^10.1.1",
    "lottie-react-native": "^7.1.0",
    "react": "18.3.1",
    "react-native": "0.76.5",
    "react-native-blob-util": "^0.21.2",
    "react-native-device-info": "^14.0.2",
    "react-native-document-picker": "^9.3.1",
    "react-native-fs": "^2.20.0",
    "react-native-gesture-handler": "^2.21.2",
    "react-native-get-random-values": "^1.11.0",
    "react-native-image-picker": "^7.2.3",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-mmkv": "^3.2.0",
    "react-native-network-info": "^5.2.1",
    "react-native-permissions": "^5.2.1",
    "react-native-qrcode-svg": "^6.3.12",
    "react-native-reanimated": "^3.16.6",
    "react-native-responsive-fontsize": "^0.5.1",
    "react-native-safe-area-context": "^5.0.0",
    "react-native-screens": "^4.4.0",
    "react-native-svg": "^15.10.1",
    "react-native-svg-transformer": "^1.5.0",
    "react-native-tcp-socket": "^6.2.0",
    "react-native-udp": "^4.1.7",
    "react-native-vector-icons": "^10.2.0",
    "react-native-vision-camera": "^4.6.3",
    "uuid": "^11.0.3",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "15.0.1",
    "@react-native-community/cli-platform-android": "15.0.1",
    "@react-native-community/cli-platform-ios": "15.0.1",
    "@react-native/babel-preset": "0.76.5",
    "@react-native/eslint-config": "0.76.5",
    "@react-native/metro-config": "0.76.5",
    "@react-native/typescript-config": "0.76.5",
    "@types/react": "^18.2.6",
    "@types/react-native-vector-icons": "^6.4.18",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "18.3.1",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
